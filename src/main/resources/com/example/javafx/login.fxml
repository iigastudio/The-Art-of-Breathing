<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="loginPane" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx.LoginController">
    <children>
        <Label layoutX="140.0" layoutY="183.0" text="User Name" />
        <Label layoutX="140.0" layoutY="242.0" text="Password" />
        <TextField fx:id="txtUserName" layoutX="245.0" layoutY="179.0" prefHeight="25.0" prefWidth="278.0" promptText="Enter UserName" />
        <PasswordField fx:id="txtPassword" layoutX="245.0" layoutY="238.0" prefHeight="25.0" prefWidth="278.0" promptText="Enter Password" />
        <Button fx:id="btnLogin" layoutX="245.0" layoutY="304.0" mnemonicParsing="false" onAction="#loginAction" text="Login" />
        <Label id="loginFormTitle" alignment="CENTER" layoutY="117.0" prefHeight="20.0" prefWidth="640.0" text="Buteyko Breathing Exercise " textFill="#150a5b">
            <font>
                <Font name="Garamond Bold" size="28.0" />
            </font>
        </Label>

    </children>
</AnchorPane>
